;;; Testing a different match let

(load "ghelper")
(load "matcher")

(define (dict-let dicts . body)
  (define (dict-let-single alist dicts body)
    (if (null? alist)
	())))

(define (dict-let-simple p . body)
  (let ((var (car p))
	(val (cadr p)))
    ((lambda (var)
       (force body))
     val)))

(dict-let-simple (list 'a 4) (delay (+ a 2)))
